#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    vector <vector<int>> vec;
    int n,q;
    cin>>n>>q;
    
    for (int a=0; a<n; a++) {
        vector<int> temp;
        int size;
        cin>>size;
        for (int b=0; b<size; b++) {
            int ele;
            cin>>ele;
            temp.push_back(ele);
        }
        vec.push_back(temp);
    }
    
    int i,j;
    for (int k = 0; k < q; k++)
    {
      cin>>i>>j;
      cout<<vec[i][j];
      cout<<endl;
    }
    
       
    return 0;
}
